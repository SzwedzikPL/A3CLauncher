{
  "name": "A3CLauncher",
  "description": "Arma3Coop.pl community game launcher for Windows",
  "private": true,
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/SzwedzikPL/A3CLauncher.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && electron-builder"
  },
  "author": {
    "name": "SzwedzikPL",
    "email": "szwedzikpl@gmail.com",
    "url": "https://arma3coop.pl"
  },
  "build": {
    "appId": "com.arma3coop.launcher",
    "copyright": "Copyright Â© 2020 SzwedzikPL",
    "productName": "A3C Launcher",
    "asar": false,
    "win": {
      "target": [
        "portable",
        "zip"
      ]
    },
    "afterAllArtifactBuild": "scripts/onBuildDone.js",
    "files": [
      "dist/**/*",
      "!node_modules/**/*"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "dependencies": {
    "bootstrap": "4.4.1",
    "electron-log": "4.1.1",
    "electron-store": "5.1.1",
    "font-awesome": "4.7.0",
    "registry-js": "1.9.0",
    "vue": "2.6.11",
    "vuex": "3.1.3"
  },
  "devDependencies": {
    "@vue/preload-webpack-plugin": "1.1.1",
    "chalk": "3.0.0",
    "css-loader": "3.4.2",
    "devtron": "1.4.0",
    "electron": "8.2.0",
    "electron-builder": "22.4.1",
    "electron-devtools-installer": "2.2.4",
    "electron-rebuild": "1.10.1",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "3.2.0",
    "node-loader": "0.6.0",
    "url-loader": "4.0.0",
    "vue-loader": "15.9.1",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.42.1",
    "webpack-dev-server": "3.10.3",
    "webpack-hot-middleware": "2.25.0"
  }
}
